import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner obj= new Scanner(System.in);
	    System.out.println("Enter a Postitve Number 1 : ");
	    int a=obj.nextInt();
	    System.out.println("Enter a Postitve Number 2 : ");
	    int b=obj.nextInt();
	    //USing Eucledian Theorem
	    int t1=a; //For Printing the Final Result 
	    int t2=b;
	    while(b!=0){
	        int temp=b;
	        b=a%b;
	        a=temp;
	    }
	    System.out.println("The HCF of "+t1+" and "+t2+" is "+a); 
	}
}
